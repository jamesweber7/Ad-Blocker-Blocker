{
    "name": "Ad Blocker Blocker",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["popups.js", "content.js"],
            "css": ["style.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["images/*", "sounds/*"],
            "matches": ["<all_urls>"]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "description": "Adds an unhealthy dose of popups to your browsing experience."
}
